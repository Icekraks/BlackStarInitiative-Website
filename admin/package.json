{
  "name": "black-star-initiative",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "pnpm run schema:typegen && sanity dev",
    "serve": "sanity start",
    "build": "sanity build",
    "deploy:admin": "sanity deploy",
    "deploy:schema": "sanity graphql deploy --no-playground --force",
    "schema:typegen": "pnpm sanity-codegen"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/table": "^1.1.2",
    "@sanity/vision": "^3.58.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "sanity": "^3.58.0",
    "sanity-codegen": "^0.9.8",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^4.0.0",
    "@types/react": "^18.0.25",
    "eslint": "^8.6.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
}
